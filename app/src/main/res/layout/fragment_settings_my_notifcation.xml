<?xml version="1.0" encoding="utf-8"?>
<com.moinapp.wuliao.widget.CustomerScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/layout_common_title_bar" />

        <LinearLayout style="@style/option_item_ll">

            <RelativeLayout
                android:id="@+id/rl_accept"
                style="@style/option_item_rl">

                <TextView
                    style="@style/option_item_text_parent"
                    android:text="@string/notice_accept"
                    android:textColor="@color/main_black"
                    android:textSize="@dimen/text_size_16" />

                <com.moinapp.wuliao.widget.togglebutton.ToggleButton
                    android:id="@+id/tb_accept"
                    android:layout_width="45dp"
                    android:layout_height="20dp"
                    android:layout_alignParentRight="true" />
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>
</com.moinapp.wuliao.widget.CustomerScrollView>